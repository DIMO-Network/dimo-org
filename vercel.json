{
  "redirects": [
    {
      "source": "/docs/api-references/conversations-api",
      "destination": "/docs/api-references/agents-api",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/docs/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600, stale-while-revalidate"
        }
      ]
    }
  ]
}
